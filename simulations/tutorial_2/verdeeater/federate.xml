<?xml version="1.0"?>

<federate name="verdeeater" system="verdeeater">

<attributes>
  <attribute name="number" varname="number" default="2" type="integer" min="0" max="1000" 
	     label="number" />
  
  <attribute name="initial energy tokens" varname="initial_tokens" default="50" type="integer" min="0" max="1000" 
	     label="initial energy tokens" />
  <attribute name="lifespan" varname="lifespan" default="20" type="integer" min="1" max="1000" 
	     label="lifespan" />

  <attribute name="reproductive age" varname="rep_age" default="3" type="integer" min="0" max="1000" 
	     label="reproductive age" />
 
</attributes>

<handlers>
  <event type="init" file="init.py"/>
  <event type="tick" file="tick.py"/>
  <event type="display" file="display.py"/>
  <event type="unload" file="unload.py"/>	
</handlers>

</federate>
